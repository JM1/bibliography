% Computational techniques by Chris Baker and Terry Johnson and Dominic Flynn and Hassan Hemida and Andrew Quinn and David Soper and Mark Sterling (2019)
@incollection{BJFHQSS19CT,
 title = {Chapter 4 - Computational techniques},
 author = {Baker, Chris and Johnson, Terry and Flynn, Dominic and Hemida, Hassan and Quinn, Andrew and Soper, David and Sterling, Mark},
 editor = {Baker, Chris and Johnson, Terry and Flynn, Dominic and Hemida, Hassan and Quinn, Andrew and Soper, David and Sterling, Mark},
 booktitle = {Train Aerodynamics},
 publisher = {Butterworth-Heinemann},
 pages = {53--71},
 year = {2019},
 isbn = {978-0-12-813310-1},
 doi = {https://doi.org/10.1016/B978-0-12-813310-1.00004-6},
 url = {http://www.sciencedirect.com/science/article/pii/B9780128133101000046},
 keywords = {CFD, DES, DNS, LES, Optimisation techniques, Panel methods, RANS, Turbulence models},
 abstract = {This chapter gives an introduction to a number of computational techniques that are used in the study of train aerodynamics. Firstly, the range of analytical and numerical techniques used in train aerodynamics is described. Simple panel methods are then outlined and their historic and continuing utility discussed. The use of the Reynolds-averaged Navier–Stokes (RANS) equations is then discussed at some length. The representation of turbulence in this approach is presented, and a range of turbulence viscosity models are discussed. Reynolds stress models and unsteady RANS methods are briefly described. Direct numerical simulation (DNS) of the Navier–Stokes equations is then considered, and it is shown that a direct application of this approach to train aerodynamics is not currently possible. Hybrid models are then discussed (large eddy simulation (LES) and detached eddy simulation (DES), which use DNS for the calculation of large turbulence scales and turbulence models of different sorts for small turbulence scales or near flow boundaries. The Lattice Boltzmann (LB) approach is then set out, which solves the Boltzmann equation rather than the Navier–Stokes equations, and is used in a similar manner to LES. Finally, a range of numerical optimisation techniques are presented.},
}

